        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 1}
      },
      {
        "type": "timeseries",
        "title": "TGI Prefill Operations",
        "datasource": "Prometheus",
        "targets": [{
          "expr": "rate(tgi_batch_inference_count{method=\"prefill\"}[1m])",
          "legendFormat": "Prefill/s"
